{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "console_title_template": "{{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#FEF5ED>\ue0b2</>",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "properties": {
            "macos": "\uf179 ",
            "ubuntu": "\uf31b ",
            "windows": "\ue62a "
          },
          "background": "#FEF5ED",
          "foreground": "#011627",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "time",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#FEF5ED>\ue0b2</>",
          "template": " â™¥ {{ .CurrentDate | date .Format }} ",
          "properties": {
            "time_format": "15:04:05"
          },
          "background": "#FEF5ED",
          "foreground": "#011627",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "sysinfo",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "template": "\ue266 CPU: {{ round .PhysicalPercentUsed .Precision }}% | ",
          "background": "#516BEB",
          "foreground": "#ffffff",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "sysinfo",
          "style": "diamond",
          "trailing_diamond": "<transparent,#516BEB>\ue0b2</>",
          "template": "RAM: {{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB \ue266 ",
          "background": "#516BEB",
          "foreground": "#ffffff",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .FormattedMs }} ",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "background": "#575656",
          "foreground": "#d6deeb",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "<transparent,#ffffff>\ue0b2</>",
          "template": "\ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "properties": {
            "fetch_package_manager": true,
            "npm_icon": " <#cc3a3a>\ue5fa</> ",
            "yarn_icon": " <#348cba>\ue6a7</>"
          },
          "background": "#ffffff",
          "foreground": "#000000",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "\ue0b2",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "\ue725 ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "background": "#17D7A0",
          "foreground": "#011627",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "path",
          "style": "diamond",
          "template": " \ue5ff {{ .Path }} ",
          "properties": {
            "folder_icon": "\uf07b",
            "home_icon": "home",
            "style": "agnoster_full"
          },
          "background": "",
          "foreground": "#ffafd2",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        },
        {
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ],
          "template": " \ueb05 ",
          "properties": {
            "always_enabled": true
          },
          "background": "",
          "foreground": "#00ff15",
          "Text": "",
          "Duration": 0,
          "NameLength": 0
        }
      ],
      "newline": true
    }
  ],
  "transient_prompt": {
    "template": "\ue285 ",
    "background": "transparent",
    "foreground": "#FEF5ED",
    "Text": "",
    "Duration": 0,
    "NameLength": 0
  }
}
